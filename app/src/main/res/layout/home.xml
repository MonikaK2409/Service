<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeActivity">

    <TextView
        android:id="@+id/textt"
        android:layout_width="258dp"
        android:layout_height="54dp"
        android:text="YOUR HOME!"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.004"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.009" />

    <Button
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Log out"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.99"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <GridLayout
        android:layout_width="273dp"
        android:layout_height="276dp"
        android:layout_marginTop="152dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="75dp"
            android:layout_row="0"
            android:layout_column="0"
            android:onClick="makeupfun"
            android:src="@drawable/makeup" />

        <TextView
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_row="1"
            android:layout_column="0"
            android:layout_marginTop="0dp"
            android:text="  Make Up"
            android:textAlignment="center" />

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="75dp"
            android:layout_row="0"
            android:layout_column="1"
            android:onClick="carpenterfun"
            android:src="@drawable/carpenter" />

        <TextView
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_row="1"
            android:layout_column="1"
            android:layout_marginTop="0dp"
            android:text="  Carpentry"
            android:textAlignment="center" />

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="75dp"
            android:layout_row="0"
            android:layout_column="2"
            android:onClick="electricianfun"
            android:src="@drawable/electrician" />

        <TextView
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_row="1"
            android:layout_column="2"
            android:layout_marginTop="0dp"
            android:text="  Electrician"
            android:textAlignment="center" />

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="75dp"
            android:layout_row="2"
            android:layout_column="0"
            android:onClick="plumberfun"
            android:src="@drawable/plumber" />

        <TextView
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_row="3"
            android:layout_column="0"
            android:layout_marginTop="0dp"
            android:text="  Plumber"
            android:textAlignment="center" />

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="75dp"
            android:layout_row="2"
            android:layout_column="1"
            android:onClick="housekeepingfun"
            android:src="@drawable/housekeeping" />

        <TextView
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_row="3"
            android:layout_column="1"
            android:layout_marginTop="0dp"
            android:text="Housekeeping"
            android:textAlignment="center" />

    </GridLayout>
</androidx.constraintlayout.widget.ConstraintLayout>